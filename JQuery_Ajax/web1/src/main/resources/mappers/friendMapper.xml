<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.kdigital.web1.mapper.FriendMapper">
	<insert id="insertFriend" parameterType="Friend">
	insert into friend
	values
	(friend_seq.nextval, #{name}, #{phone}, #{address})
	</insert>
	
	<select id="selectFriendAll" resultType="Friend">
	select * from friend
	</select>
</mapper>
















